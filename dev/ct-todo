CREATE TABLE IF NOT EXISTS `log` (
  `i` int(11) NOT NULL AUTO_INCREMENT,
  `j` varchar(128) NOT NULL DEFAULT '0f',
  `k` varchar(16) NOT NULL DEFAULT 'UV',
  `v_uid` int(11) unsigned NOT NULL DEFAULT '0',
  `v_ip4i` int(11) unsigned NOT NULL DEFAULT '0',
  `v_ip4` varchar(16) NOT NULL DEFAULT '127.0.0.1',
  `v_host` varchar(64) NOT NULL DEFAULT 'UNKNOWN',
  `v_cc` varchar(3) NOT NULL DEFAULT 'XX',
  `v_dev` varchar(64) NOT NULL DEFAULT 'UNKNOWN',
  `v_ua` varchar(128) NOT NULL DEFAULT 'UNKNOWN',
  `t` int(11) unsigned NOT NULL DEFAULT '0',
  `u` int(11) unsigned NOT NULL DEFAULT '0',
  `e` int(11) unsigned NOT NULL DEFAULT '0',
  PRIMARY KEY (`i`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

